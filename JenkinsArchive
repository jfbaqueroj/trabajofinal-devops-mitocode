pipeline {
	agent any
	
	environment{
		
	}
	
	stages{

		stage('Archivar el JAR') {
			   steps {
                sh 'mkdir Archivo_JAR'
                sh 'echo "not an artifact file" > js/build.js'
                sh 'echo "artifact file" > js/build.min.js'
                
                sh 'mkdir css'
                sh 'echo "not a artifact file" > css/build.css'
                sh 'echo "artifact file" > css/build.min.css'
            }
		  }
		}
	}
}